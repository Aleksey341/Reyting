# Production environment variables for Vite build
# These are embedded during 'npm run build'

# API Base URL - use relative path so Nginx proxy handles it
# This enables same-origin requests without CORS complexity
# Nginx will proxy /api/* to the actual backend
VITE_API_BASE=/api

# Application config
VITE_APP_TITLE=Reyting Dashboard

# Debug mode (disabled in production)
VITE_DEBUG=false
